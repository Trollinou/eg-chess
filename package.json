{
  "name": "eg-chess",
  "version": "1.0.0",
  "description": "A Javascript library for chess.",
  "main": "dist/eg-chess.js",
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:js": "esbuild src/eg-chess/EGChess.js --bundle --outfile=dist/eg-chess.js --format=esm --external:cm-chessboard --external:chess.js",
    "build:css": "sass --quiet-deps --silence-deprecation=import src/scss/eg-chess.scss dist/eg-chess.css",
    "start": "http-server -c-1"
  },
  "keywords": [
    "chess"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cpx2": "^8.0.0",
    "esbuild": "^0.27.0",
    "http-server": "^14.1.1",
    "sass": "^1.94.2"
  }
}
